{% # theme-check-disable %}
{% comment %}
  IMPORTANT: This snippet is automatically generated by Vite.
  Do not attempt to modify this file directly, as any changes will be overwritten by the next build.
{% endcomment %}
{% assign path = adastra | replace: '~/', '../' | replace: '@/', '../' %}
{% if path == "/src/entrypoints/alpine.js" or path == "alpine.js" %}
  <link rel="modulepreload" href="{{ 'alpine.js' | asset_url | split: '?' | first }}" as="script" crossorigin="anonymous">
  <script src="{{ 'alpine.js' | asset_url | split: '?' | first }}" type="module" crossorigin="anonymous"></script>
{% elsif path == "/src/entrypoints/base.css" or path == "base.css" %}
  {{ 'base.css' | asset_url | split: '?' | first | stylesheet_tag: preload: preload }}
{% elsif path == "/src/entrypoints/index.js" or path == "index.js" %}
  <link rel="modulepreload" href="{{ 'index.js' | asset_url | split: '?' | first }}" as="script" crossorigin="anonymous">
  <script src="{{ 'index.js' | asset_url | split: '?' | first }}" type="module" crossorigin="anonymous"></script>
  <link rel="modulepreload" href="{{ 'splide.js' | asset_url | split: '?' | first }}" as="script" crossorigin="anonymous">
  {{ 'splide.css' | asset_url | split: '?' | first | stylesheet_tag: preload: preload }}
  <link rel="modulepreload" href="{{ 'alpine.js' | asset_url | split: '?' | first }}" as="script" crossorigin="anonymous">
{% elsif path == "/src/entrypoints/splide.js" or path == "splide.js" %}
  <link rel="modulepreload" href="{{ 'splide.js' | asset_url | split: '?' | first }}" as="script" crossorigin="anonymous">
  <script src="{{ 'splide.js' | asset_url | split: '?' | first }}" type="module" crossorigin="anonymous"></script>
  {{ 'splide.css' | asset_url | split: '?' | first | stylesheet_tag: preload: preload }}
{% endif %}
